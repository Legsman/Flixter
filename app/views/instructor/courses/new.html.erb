<br />
<div class="booyah-box col-xs-10 col-xs-offset-1">
  <div class="meows">
    <h1><strong>Add a new course</strong></h1>
  </div>
    <%= simple_form_for @course, url: instructor_courses_path do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :cost %>
    <%= f.input :image %>
    <br />
  <div class="meows">
    <%= f.submit "Create", class: 'btn btn-success' %>
  </div>
    <br />
  <% end %>
  <br />
</div>